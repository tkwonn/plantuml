{
    "name": "tkwonn/plantuml",
    "description": "PlantUML Server App",
    "license": "proprietary",
    "scripts": {
        "serve": "php -S localhost:8000 -t public/",
        "cs-fix": "php-cs-fixer fix --diff"
    },
    "autoload": {
        "psr-4": {
            "Exceptions\\": "src/Exceptions/",
            "Helpers\\": "src/Helpers/",
            "Models\\": "src/Models/",
            "Response\\": "src/Response/",
            "Views\\": "src/Views/"
        }
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.64"
    }
}
